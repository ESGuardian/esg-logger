{
  "order": 0,
  "template": "oracle*",
  "settings": {
    "index.refresh_interval": "5s"
  },
  "mappings": {
    "oracle": {
      "dynamic_templates": [
        {
          "notanalyzed": {
            "match": "*",
            "match_mapping_type": "string",
            "mapping": {
              "type": "string",
              "index": "not_analyzed",
              "doc_values": "true"
            }
          }
        }
      ],
      "properties": {        
        "tags": {
          "type": "string"
        },
        "comment_text": {
          "type": "string"
        }, 
        "sql_text": {
          "type": "string"
        },
        "@timestamp": {
          "type": "date",
          "format": "dateOptionalTime",
          "index": "not_analyzed"
        },
        "timestamp": {
          "type": "date",
          "format": "dateOptionalTime",
          "index": "not_analyzed"
        },
        "extended_timestamp": {
          "type": "date",
          "format": "dateOptionalTime",
          "index": "not_analyzed"
        },
        "@version": {
          "type": "string"
        }
      }
    }
  }
}

